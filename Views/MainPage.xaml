<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Carnet_Bebe.Components"
             x:Class="CarnetBebe.View.MainPage"
             Title="MainPage">

    <ScrollView>
        <VerticalStackLayout>
            <local:ButtonsImageGroup
            HeaderImage="angrypoo.png"
            Button1Image="angrypoo.png"
            Button2Image="angrypoo.png"
            Button1Command="{Binding Button1Command}"
            Button2Command="{Binding Button2Command}"
            />

            <local:ButtonsAndLine />
            <local:ButtonsImageGroup
            HeaderImage="angrypoo.png"
            Button1Image="angrypoo.png"
            Button2Image="angrypoo.png"
            Button1Command="{Binding Button1Command}"
            Button2Command="{Binding Button2Command}"/>
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>
